SQLite database setup



create table wishlistItems(
wishlistID      text,
itemID          text,
item_url        text,
itemTitle       text,
starRating      text,
fullImageURL    text,
thumbnailURL    text,
);


create table items (
itemID             text,
fullPrice          text,
itemType           text, # hardcover, kindle, etc
usedPrice          text,
newPrice           text,
collectiblePrice   text,
itemTypePrice      text, 
primeStatus        text,
categories         text,

underABuck         text,
priceDiff          text
);   


